
root = true

[*]
charset = utf-8
tab_width = 4
indent_size = 4
indent_style = tab
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.csproj]
indent_size = 2
indent_style = space

[*.json]
indent_size = 2
indent_style = space

[*.yaml]
indent_size = 2
indent_style = space
